    "@testing-library/react": "^11.2.6",
    "@testing-library/user-event": "^13.1.8",
    "@types/jest": "^26.0.23",
    "@types/react": "^17.0.5",
    "babel-jest": "^26.6.3",
    "jest": "^26.6.3",
    "next": "^10.0.0",
